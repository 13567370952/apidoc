{
  "name": "apidoc",
  "version": "0.30.0",
  "description": "RESTful web API Documentation Generator",
  "author": "Peter Rottmann <rottmann@inveris.de>",
  "license": "MIT",
  "preferGlobal": true,
  "bin": "bin/apidoc",
  "main": "./lib/index.js",
  "homepage": "https://apidocjs.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/apidoc/apidoc.git"
  },
  "bugs": {
    "url": "https://github.com/apidoc/apidoc/issues"
  },
  "scripts": {
    "test": "mocha test/",
    "eslint": "./node_modules/.bin/eslint lib",
    "build-example": "./bin/apidoc -v -i example/ -o /tmp/apidoc-output",
    "update-lodash": "./node_modules/lodash-cli/bin/lodash -p -o template/vendor/lodash.custom.min.js include=get,set,groupBy,each,extend,some,isNaN exports=amd",
    "serve": "npm run build-example && docker run --rm --name apidoc -p 8080:80 -v /tmp/apidoc-output:/usr/share/nginx/html:ro -d nginx"
  },
  "keywords": [
    "api",
    "apidoc",
    "doc",
    "documentation",
    "rest",
    "restful"
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "dependencies": {
    "commander": "^2.20.0",
    "fs-extra": "^9.0.1",
    "handlebars": "^4.7.7",
    "iconv-lite": "^0.6.3",
    "jquery": "3.5.1",
    "klaw-sync": "^6.0.0",
    "lodash": "^4.17.20",
    "markdown-it": "^11.0.0",
    "nodemon": "^2.0.4",
    "requirejs": "^2.3.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "amdefine": "^1.0.1",
    "apidoc-example": "^0.2.3",
    "bootstrap": "3.4.1",
    "eslint": "^7.32.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "lodash-cli": "^4.17.5",
    "mocha": "^9.1.1",
    "path-to-regexp": "^3.2.0",
    "semver": "^7.3.2",
    "webfontloader": "^1.6.28"
  }
}
